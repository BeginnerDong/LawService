
.big-box{
	box-sizing: border-box;
	padding: 40rpx 30rpx;
	width: 100%;
}

.text{
	text-align: justify;
	line-height: 36rpx;
	margin: 30rpx 0rpx;
}
.other{
	width: 120rpx;
	height: 36rpx;
	border: solid 1rpx #00A1E9;
	text-align: center;
	line-height: 36rpx;
	margin-left: 40rpx;
}
.he10{
	width: 100%;
	height: 10rpx;
}
.reveiew{
	width: 100%;
	box-sizing: border-box;
	padding: 50rpx 30rpx;
}
.star{
	margin-left: 40rpx;
}
.star image{
	width: 32rpx;
	height: 32rpx;
	margin-left: 20rpx;
}


button{
	width: 80%;
	height: 80rpx;
	margin: 120rpx auto 50rpx;
}
textarea{
	box-sizing: border-box;
	padding: 30rpx;
	width: 100%;
	height: 100%;
}