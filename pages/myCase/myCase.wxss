page{
	background: #F5F5F5;
}
.big-box{
	box-sizing: border-box;
	padding: 40rpx 30rpx;
	width: 92%;
	margin: 4%;
	position: relative;
}
.right{
	position: absolute;
	right: 10rpx;
	top: 10rpx;
}
.text{
	text-align: justify;
	line-height: 36rpx;
	margin: 30rpx 0rpx;
}
.other{
	width: 120rpx;
	height: 36rpx;
	border: solid 1rpx #00A1E9;
	text-align: center;
	line-height: 36rpx;
	margin-left: 40rpx;
}
.btm{
	text-align: right;
	margin-top: 30rpx;
}
.btm button{
	height: 50rpx;
	line-height: 50rpx;
}

.fixd{
	position: fixed;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.6);
	top: 0rpx;
}
.fixd-box{
	width:90%;
	height: 400rpx;
	margin: 40% 5%;
	box-sizing: border-box;
	padding: 30rpx;
}
.fixd-box button{
	display: inline-block;
	border: solid 1rpx #00A1E9;
	width: 120rpx;
	height: 50rpx;
	background: #fff;
	color: rgb(102,102,102);
	line-height: 50rpx;
	margin-left: 21%;
	margin-top: 80rpx;
}
.star{
	background: #00A1E9 !important;
	color: #fff !important;
}