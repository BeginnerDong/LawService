
.info{
	width: 100%;
	box-sizing: border-box;
	padding:30rpx;
	border-bottom: solid 1rpx #F5F5F5;
}
.info-left{
	width: 18.5%;
	height:166rpx;
}
.info-left image{
	width: 100%;
	height:100%;
}
.info-right{
	height: 166rpx;
	width: 77%;
}
.info-item{
	width:110rpx;
	height: 40rpx;
	border: solid 1rpx #E7E7E7;
	text-align: center;
	line-height: 40rpx;
	margin-right: 20rpx;
}



